{
  "name": "@qwexs/kysely-bun-psql",
  "version": "1.0.0",
  "description": "Kysely dialect for PostgreSQL using Bun's native SQL client",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qwexs/kysely-bun-psql.git"
  },
  "homepage": "https://github.com/qwexs/kysely-bun-psql",
  "author": "qwexs <qwexlock@gmail.com>",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "exports": {
    ".": {
      "bun": "./dist/index.js",
      "require": "./dist/index.js",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "kysely",
    "bun",
    "psql",
    "postgresql",
    "dialect"
  ],
  "scripts": {
    "build": "bun --bun run build.config.ts",
    "fmt": "prettier --write .",
    "fmt:check": "prettier --check .",
    "lint": "eslint src",
    "prepack": "bun --bun run lint && bun --bun run build",
    "test": "bun --bun test --preload ./tests/setup.ts",
    "test:dialect": "bun test tests/dialect.test.ts",
    "test:ci": "bun --bun test",
    "type-check": "tsc --noEmit",
    "version:patch": "npm version patch -m 'Release v%s'",
    "version:minor": "npm version minor -m 'Release v%s'",
    "version:major": "npm version major -m 'Release v%s'"
  },
  "peerDependencies": {
    "kysely": ">= 0.24.0 < 1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/bun": "^1.3.8",
    "eslint": "^9.39.2",
    "kysely": "^0.28.11",
    "prettier": "^3.8.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0"
  },
  "sideEffects": false,
  "directories": {
    "test": "tests"
  },
  "bugs": {
    "url": "https://github.com/qwexs/kysely-bun-psql/issues"
  }
}
